{
  "name": "@miguelvaladez/typesaurus-rxjs",
  "version": "1.2.0",
  "description": "Type-safe ORM for Firestore",
  "keywords": [
    "Firebase",
    "Firestore",
    "TypeScript"
  ],
  "scripts": {
    "build-lib": "yarn build && yarn prettier && yarn copy && yarn sync",
    "build": "rm -rf lib && tsc",
    "prettier": "prettier 'lib/**/*.[jt]s' --write --loglevel silent",
    "copy": "cp {package.json,*.md} lib",
    "sync": "rsync --archive --prune-empty-dirs --exclude '*.ts' --relative src/./ lib"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "main": "index.js",
  "repository": "https://github.com/miguelvaladez/typesaurus-rxjs",
  "author": "Sasha Koss <koss@nocorp.me>",
  "contributors": [
    "Miguel Valadez <miguelvaladez@principlepms.com.au>"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-typescript": "^7.3.3",
    "@google-cloud/firestore": "^2.3.0",
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.4",
    "@types/sinon": "^7.0.13",
    "@types/webpack-env": "^1.14.0",
    "babel-loader": "^8.0.6",
    "babel-preset-power-assert": "^3.0.0",
    "firebase": "^6.6.2",
    "firebase-admin": "^8.6.0",
    "jest": "^24.8.0",
    "karma": "^4.2.0",
    "karma-chrome-launcher": "^3.0.0",
    "karma-jasmine": "^2.0.1",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^4.0.2",
    "mocha": "^6.2.0",
    "nanoid": "^2.0.3",
    "power-assert": "^1.6.1",
    "prettier": "^1.17.1",
    "puppeteer": "^1.19.0",
    "rxjs": "^6.5.3",
    "sinon": "^7.3.2",
    "typedoc": "^0.15.0",
    "typescript": "^3.6.0-beta",
    "webpack": "^4.37.0"
  },
  "peerDependencies": {
    "@google-cloud/firestore": "^2",
    "firebase": "^6",
    "firebase-admin": "^8"
  }
}
